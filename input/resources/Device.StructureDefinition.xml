<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="Device" />
  <url value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Device" />
  <name value="Device" />
  <title value="设备" />
  <status value="draft" />
  <description value="此资源是管理资源，可跟踪设备的各个实例及其位置。其他资源引用该记录，以记录哪个设备执行了操作（例如程序或观察），在处方和分配供患者使用或订购耗材的设备时引用，并用于记录和传输有关设备的唯一设备标识符（UDI）信息。" />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
  </mapping>
  <mapping>
    <identity value="w5" />
    <uri value="http://hl7.org/fhir/fivews" />
    <name value="FiveWs Pattern Mapping" />
  </mapping>
  <mapping>
    <identity value="udi" />
    <uri value="http://fda.gov/UDI" />
    <name value="UDI Mapping" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Device" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Device" />
  <derivation value="constraint" />
  <differential>
    <element id="Device">
      <path value="Device" />
      <short value="设备" />
      <definition value="此资源是管理资源，可跟踪设备的各个实例及其位置。其他资源引用该记录，以记录哪个设备执行了操作（例如程序或观察），在处方和分配供患者使用或订购耗材的设备时引用，并用于记录和传输有关设备的唯一设备标识符（UDI）信息。" />
    </element>
    <element id="Device.identifier">
      <path value="Device.identifier" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="type.coding.system" />
        </discriminator>
        <rules value="open" />
      </slicing>
      <short value="业务ID" />
      <definition value="描述设备的业务ID，由制造商、其他组织或所有者分配给设备的唯一实例标识符。" />
      <comment value="业务ID不是资源服务器上的逻辑ID（非标识主键）" />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Identifier" />
      </type>
    </element>
    <element id="Device.identifier:deviceIdentifier">
      <path value="Device.identifier" />
      <sliceName value="deviceIdentifier" />
      <short value="设备业务编码" />
    </element>
    <element id="Device.identifier:otherIdentifier">
      <path value="Device.identifier" />
      <sliceName value="otherIdentifier" />
      <short value="其他编码" />
    </element>
    <element id="Device.definition">
      <path value="Device.definition" />
      <short value="设备定义" />
      <definition value="设备的定义，对设备定义的引用" />
      <max value="0" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/DeviceDefinition" />
      </type>
    </element>
    <element id="Device.udiCarrier">
      <path value="Device.udiCarrier" />
      <short value="UDI" />
      <definition value="唯一设备标识符（UDI）条形码字符串" />
      <comment value="UDI可以标识设备的唯一实例，也可以仅标识设备的类型。参见[UDI映射](Device -mapping .html# UDI)获取UDI部分到设备的完整映射。" />
    </element>
    <element id="Device.status">
      <path value="Device.status" />
      <short value="设备状态" />
      <definition value="用编码形式表示此设备当前状态。通常指活动状态或完成状态。活跃 不活跃| 输入错误| 未知的&#xD;&#xA;若此元素值的改变会引起整个设备发生改变。如：设备运行、已停止等。" />
    </element>
    <element id="Device.status.extension">
      <path value="Device.status.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Device.status.extension:status">
      <path value="Device.status.extension" />
      <sliceName value="status" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/resource-status" />
      </type>
    </element>
    <element id="Device.status.extension:status.value[x]">
      <path value="Device.status.extension.value[x]" />
      <short value="设备状态" />
      <definition value="active | inactive | entered-in-error | unknown" />
      <binding>
        <strength value="required" />
        <description value="设备状态" />
        <valueSet value="http://fhir.scwjxx.cn/Sichuan/ValueSet/device-status-vs" />
      </binding>
    </element>
    <element id="Device.statusReason">
      <path value="Device.statusReason" />
      <definition value="用编码形式表示导致设备状态的原因，在设备为异常时使用。 在线|暂停|待机|离线|未就绪|转接| 硬件断开|关闭" />
      <max value="0" />
    </element>
    <element id="Device.distinctIdentifier">
      <path value="Device.distinctIdentifier" />
      <short value="全球唯一编码" />
      <definition value="描述该设备全球唯一编码，独特的识别字符串" />
    </element>
    <element id="Device.manufacturer">
      <path value="Device.manufacturer" />
      <short value="厂商名称" />
      <definition value="描述设备制造商名称，字符串不能大于1M" />
    </element>
    <element id="Device.manufactureDate">
      <path value="Device.manufactureDate" />
      <short value="制造时间" />
      <definition value="制造设备的日期" />
    </element>
    <element id="Device.expirationDate">
      <path value="Device.expirationDate" />
      <short value="设备到期时间" />
      <definition value="该设备的到期日期和时间（如果有）" />
      <max value="0" />
    </element>
    <element id="Device.lotNumber">
      <path value="Device.lotNumber" />
      <short value="制造批号" />
      <definition value="描述设备的制造批号，字符串不能大于1M" />
      <max value="0" />
    </element>
    <element id="Device.serialNumber">
      <path value="Device.serialNumber" />
      <short value="SN编码" />
      <definition value="描述设备的SN编号，字母或数字最大20" />
    </element>
    <element id="Device.deviceName">
      <path value="Device.deviceName" />
      <short value="设备名称" />
      <definition value="制造商提供的设备名称" />
    </element>
    <element id="Device.deviceName.name">
      <path value="Device.deviceName.name" />
      <short value="设备名称" />
      <definition value="描述设备名称" />
    </element>
    <element id="Device.deviceName.type">
      <path value="Device.deviceName.type" />
      <definition value="The type of deviceName.&#xA;UDILabelName | UserFriendlyName | PatientReportedName | ManufactureDeviceName | ModelName.&#xD;&#xA;用编码形式表示设备名称的分类。 UDI标签名称|用户友好名称|患者报告的姓名|生产商名称|型号名称。" />
    </element>
    <element id="Device.modelNumber">
      <path value="Device.modelNumber" />
      <short value="设备型号" />
      <definition value="描述设备厂商定义的该设备的设备的型号" />
    </element>
    <element id="Device.partNumber">
      <path value="Device.partNumber" />
      <short value="设备部件号" />
      <definition value="描述设备的部件号，字母或者数字最大不超过20" />
      <max value="0" />
    </element>
    <element id="Device.type">
      <path value="Device.type" />
      <short value="设备类型" />
      <definition value="The kind or type of device.&#xD;&#xA;描述设备的种类或类型（值集）" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="DeviceType" />
        </extension>
        <strength value="example" />
        <valueSet value="http://fhir.scwjxx.cn/Sichuan/ValueSet/device-type" />
      </binding>
    </element>
    <element id="Device.specialization">
      <path value="Device.specialization" />
      <short value="功能信息" />
      <definition value="描述设备支持的功能，设备用于特定目的的标准以及用于通信的功能" />
    </element>
    <element id="Device.specialization.systemType">
      <path value="Device.specialization.systemType" />
      <short value="功能" />
      <definition value="描述用于操作和通讯的标准" />
    </element>
    <element id="Device.specialization.version">
      <path value="Device.specialization.version" />
      <short value="标准版本" />
      <definition value="用于操作和通信的标准版本。" />
    </element>
    <element id="Device.version">
      <path value="Device.version" />
      <short value="版本信息" />
      <definition value="描述 设备的实际设计或设备上运行的软件版本" />
      <max value="0" />
    </element>
    <element id="Device.property">
      <path value="Device.property" />
      <short value="配置信息" />
      <definition value="设备实际运行时的实际配置设置，例如调节状态，时间属性" />
      <max value="0" />
    </element>
    <element id="Device.property.type">
      <path value="Device.property.type" />
      <definition value="指定属性DeviceDefinitionPropetyCode(可扩展)的代码。" />
    </element>
    <element id="Device.property.valueCode">
      <path value="Device.property.valueCode" />
      <definition value="属性值作为代码，例如，NTP4(同步到NTP)。" />
    </element>
    <element id="Device.patient">
      <path value="Device.patient" />
      <short value="Patient to whom Device is affixed  设备安装的患者" />
      <definition value="Patient information, If the device is affixed to a person.&#xD;&#xA; 描述安装该设备的患者的引用，如果该设备已经被安装到患者体内，或者和患者在使用，就需要关联患者信息" />
      <max value="0" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Patient" />
      </type>
    </element>
    <element id="Device.owner">
      <path value="Device.owner" />
      <short value="机构信息" />
      <definition value="描述负责设备的提供和持续维护的组织机构的引用，在这里一般指医院/科室等信息" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Organization" />
      </type>
    </element>
    <element id="Device.contact">
      <path value="Device.contact" />
      <short value="联系信息" />
      <definition value="描述负责该设备的组织或特定人员的联系信息，包括电话，电子邮件等，一般用于排除故障" />
      <max value="0" />
      <type>
        <code value="ContactPoint" />
        <profile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/ContactPoint" />
      </type>
    </element>
    <element id="Device.location">
      <path value="Device.location" />
      <short value="位置信息" />
      <definition value="描述该设备的位置地点，包括诊室、检查室、病床、病房等。" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Location" />
      </type>
    </element>
    <element id="Device.url">
      <path value="Device.url" />
      <short value="访问设备的地址" />
      <definition value="描述 联系设备的网络地址，可以是一个url" />
      <max value="0" />
    </element>
    <element id="Device.note">
      <path value="Device.note" />
      <definition value="描述设备说明和注释" />
      <type>
        <code value="Annotation" />
        <profile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Annotation" />
      </type>
    </element>
    <element id="Device.safety">
      <path value="Device.safety" />
      <short value="安全描述" />
      <definition value="描述有关医疗设备的附加安全特性。例如含有乳胶的设备（值集）" />
      <max value="0" />
    </element>
    <element id="Device.parent">
      <path value="Device.parent" />
      <short value="The parent device  父设备信息" />
      <definition value="The parent device.&#xD;&#xA;该设备的父设备（当该设备为父设备的一个组成部分时）" />
      <max value="0" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.scwjxx.cn/SiChuan/StructureDefinition/Device" />
      </type>
    </element>
  </differential>
</StructureDefinition>